<IfModule mod_rewrite.c>
    RewriteEngine on
    # SEO redirect
    RewriteBase /
    RewriteCond %{HTTP_HOST} ^www.boxmeupapp.com$ [NC]
    RewriteRule ^(.*)$ http://boxmeupapp.com/$1 [R=301,L]

    RewriteRule    ^$    app/webroot/    [L]
    RewriteRule    (.*) app/webroot/$1    [L]
 </IfModule>
